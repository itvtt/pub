    "df.head()\n",
    "\n",
    "df_melted = df.melt(value_name='Value')\n",
    "print(df_melted['Value'])\n",
    "\n",
    "value_counts = df_melted['Value'].value_counts()\n",
    "print(value_counts)\n",
    "value_counts.to_excel('./lotto2.xlsx')\n"
